$che-loader-bottom-bar-link-color = #a5a5a5

.che-loader .che-loader-bottom-bar
  border-top 10px solid #31487b
  background-color #1c253f
  position relative
  bottom 0
  left 0
  height 200px
  width 100%
  padding-top 80px
  z-index 1

  .che-link
    color $che-loader-bottom-bar-link-color
    text-decoration none
